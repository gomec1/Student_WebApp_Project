{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "vorname": {
      "type": "string",
      "required": false
    },
    "name": {
      "type": "string",
      "required": false
    },
    "geburtsdatum": {
      "type": "string",
      "required": false
    },
    "strasseNr": {
      "type": "string",
      "required": false
    },
    "ort": {
      "type": "string",
      "required": false
    },
    "ausweis": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "profilfoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ]
    },
    "rolle": {
      "type": "enumeration",
      "enum": [
        "Ich bin Tiersitter",
        "Ich bin Tierbesitzer",
        "beides"
      ],
      "required": false
    },
    "postleitzahl": {
      "type": "biginteger"
    },
    "inserat_ich_bin_tiersitters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inserat-ich-bin-tiersitter.inserat-ich-bin-tiersitter",
      "mappedBy": "user"
    },
    "inserat_ich_bin_tierbesitzers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inserat-ich-bin-tierbesitzer.inserat-ich-bin-tierbesitzer",
      "mappedBy": "users_permissions_user"
    },
    "telefonnummer": {
      "type": "customField",
      "options": {
        "country": "ch"
      },
      "customField": "plugin::strapi-phone-validator.phone"
    }
  }
}
